<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
        "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
        "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <definition name="defaultTemplate" template="/WEB-INF/views/page.jsp">
        <put-attribute name="header" value="/WEB-INF/views/header.jsp" />
        <put-attribute name="footer" value="/WEB-INF/views/footer.jsp" />
        <!--跳转页面-->
        <put-attribute name="jump" value="profession"/>
    </definition>
    <definition name="home" extends="defaultTemplate">
        <put-attribute name="body" value="/WEB-INF/views/home.jsp" />

    </definition>
    <definition name="profession" extends="defaultTemplate">
        <put-attribute name="body" value="/WEB-INF/views/profession.jsp" />
        <!--跳转页面-->
        <put-attribute name="jump" value="home"/>
    </definition>
    <definition name="company" extends="defaultTemplate">
        <put-attribute name="body" value="/WEB-INF/views/company.jsp" />
        <!--跳转页面-->
        <put-attribute name="jump" value="home"/>
    </definition>
</tiles-definitions>